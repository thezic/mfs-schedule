<html>
	<head>
		<meta charset='utf-8' />
	</head>
	<body>
		<h1>{{month}}</h1>
		<div>{{text}}</div>
		<table>
			<tbody>
				{{#each events as |event|}}
					<tr>
						<td>{{format event.date '%d.%m'}}</td>
						<td>{{event.time}}</td>
						<td>{{event.assigneeName}}</td>
						<td>{{event.place}}</td>
						<td>{{event.extraInfo}}</td>
					</tr>
				{{/each}}
			</tbody>
		</table>
	</body>
</html>
